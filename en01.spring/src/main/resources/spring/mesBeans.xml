<?xml version="1.0" encoding="UTF-8"?>
<beans	xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Declarez vos bean ici -->
	
	<bean id="monId" class="com.exo.Client">
		<property name="nom" value="Malandain"/>
		<property name="prenom" value="Jacques"/>
		<property name="age" value="48"/>
	</bean>
	
	
	<bean id="monadresse" class="com.exo.Adresse">
		<property name="codePostal" value="59800"/>
		<property name="rue" value="Rue Monneaux"/>
		<property name="ville" value="Lille"/>
		<property name="pays" value="France"/>
	</bean>
	
	
	

</beans>
